{
  "manifest_version": 2,

  "name": "CSE Finder",
  "description": "Consolidates Pod Travel CSE queues into one view",
  "version": "1.0.0",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ]
}