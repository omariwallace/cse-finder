{
  "manifest_version": 2,

  "name": "CE Finder",
  "description": "Consolidates Pod US CE queues into one view",
  "version": "1.3.1",
  "icons": {
    "16": "murca16.png",
    "48": "murca48.png",
    "128": "murca128.png"
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ]
}